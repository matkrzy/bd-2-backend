web: java $JAVA_OPTS -Dserver.port=$PORT -Dspring.server.port=server.port=${PORT:9090} -Dspring.server.servlet.context-path=$SERVER_SERVLET_CONTEXT_PATH -Dspring.datasource.url=$DATA_SOURCE_URL -Dspring.datasource.username=$DATA_SOURCE_USER -Dspring.datasource.password=$DATA_SOURCE_PASS -Dspring.datasource.initialization-mode=$INIT_MODE -Daws.endpointUrl=$AWS_ENDPOINT_URL -Daws.accessKey=$AWS_ACCESS_KEY -Daws.secretKey=$AWS_SECRET_KEY -Daws.bucketName=$AWS_BUCKET_NAME -Dspring.jpa.show-sql=$SHOW_SQL -jar target/api-0.0.1-SNAPSHOT.jar